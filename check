
    public static Object convert(String field) {
        TypeInferer type = TypeInferer.infer(field);

        if (type == TypeInferer.STRING) {
            return field;
        } else if (type == TypeInferer.INT) {
            return Integer.parseInt(field);
        } else if (type == TypeInferer.DOUBLE) {
            return Double.parseDouble(field);
        } else if (type == TypeInferer.TAG) {
            return new GroovyTag(1); //TODO
        }

        return null;
    }